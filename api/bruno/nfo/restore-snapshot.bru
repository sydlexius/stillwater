meta {
  name: Restore Snapshot
  type: http
  seq: 4
}

post {
  url: {{apiBase}}/artists/{{artistId}}/nfo/snapshots/{{snapshotId}}/restore
  body: none
  auth: none
}

headers {
  Cookie: session={{sessionToken}}
}

tests {
  test("should return 200", function() {
    expect(res.status).to.equal(200);
  });

  test("should return restored status", function() {
    expect(res.body.status).to.equal("restored");
  });
}
